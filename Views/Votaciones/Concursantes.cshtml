@model IEnumerable<Concursante>
@{
    ViewData["Title"] = "Concursante";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h1> Lista de Concursantes </h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre de la Concursante</th>
                <th>Votos</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var concursante in Model)
            {
                <tr>
                    <td>@concursante.ConcursanteId</td>
                    <td>@concursante.Concursantes</td>
                    <td>@concursante.Votos</td>
                </tr>
            }
        </tbody>
    </table>
